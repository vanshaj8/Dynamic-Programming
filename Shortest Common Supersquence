#include<bits/stdc++.h>
#include<algorithm>
using namespace std;

int lcs(char* x ,char* y,int n,int m)
{
	int  dp[n+1][m+1];
	
	for(int i=0;i<=n;i++)
	{
		for(int j=0;j<=m;j++)
		{
			if(i==0 || j==0)
			{
				dp[i][j]=0;
			}
			else if(x[i-1]==y[j-1])
			{
				dp[i][j]=dp[i-1][j-1]+1;
			}
			else
			{
				dp[i][j]=max(dp[i-1][j],dp[i][j-1]);
			}
		}
	}

return (n+m-dp[n][m]);
}
int main()
{
	char X[] = "AGGTAB"; 
  char Y[] = "GXTXAYB"; 
  int m = strlen(X); 
  int n = strlen(Y); 
  cout<<lcs(X, Y, m, n); 
  return 0;
	
}
